<div id="main-content">
    <!--Main Content-->
    <!-- Category -->
    <div class="container-fluid">
        <div class="row mb-3 bg-secondary ">
            <div class="col-lg-1"></div>
            <div class="col-lg-10">
                <ul class="nav justify-content-center ">
                    {{#each this.category}}
                        <li class="nav-item">
                            <a class="nav-link text-white" href="#">{{CatName}}</a>
                        </li>
                    {{/each}}
                </ul>
            </div>
            <div class="col-lg-1"></div>
        </div>
    </div>
    <!-- End of Category -->

    <!-- Top End -->
    <div class="container-fluid py-3">
        <div class="row">
            <div class="col-lg-1"></div>
            <div class="col-lg-10">

                <div class="card">
                    <div class="card-header">
                        <div class="d-flex justify-content-between">
                            <h4 class="">End soon!</h4>
                            <a class="text-success font-weight-bold ml-auto mr-0" href="#">See all
                                <i class="fa fa-arrow-right" aria-hidden="true"></i></a>
                        </div>
                    </div>


                    <div class="card-body">
                        <div class="card-deck">
                            <div id="carouselExampleCaptions" class="carousel slide" data-ride="carousel">
                                <ol class="carousel-indicators">
                                    <li data-target="#carouselExampleCaptions" data-slide-to="0" class="active"></li>
                                    <li data-target="#carouselExampleCaptions" data-slide-to="1"></li>
                                </ol>
                                <div class="carousel-inner">
                                    {{#each this.top_end}}
                                        <div class="carousel-item active">
                                            <div class="card card-small col-lg-4">
                                                <div class="thumbnail">
                                                    <img class="card-img-top"
                                                         src="./public/imgs/sp/{{ProID}}/main_thumbs.jpg"
                                                         alt="{{ProName}}"
                                                         title="{{ProName}}">
                                                    <span class="tag-picture">Hot</span>
                                                    <div class="details">
                                                        <div class="user">
                                                            <div class="user-photo">
                                                                <img alt="Thumb"
                                                                     src="https://bootdey.com/img/Content/avatar/avatar1.png">
                                                            </div>
                                                            <div class="name">User</div>
                                                        </div>
                                                        <div class="numbers">
                                                            <b class="downloads"><i
                                                                    class="fa fa-arrow-circle-o-down"></i> 1124</b>
                                                            <b class="comments-icon"><i class="fa fa-comment"></i>
                                                                10</b>
                                                        </div>
                                                        <div class="clearfix"></div>
                                                    </div>
                                                </div>

                                                <div class="card-body">

                                                    <h5 class="card-title">{{ProName}}</h5>
                                                    <h5 class="card-title text-success">{{{format_number Price}}}</h5>

                                                    <p class="card-text">This is a wider card with supporting text below
                                                        as a
                                                        natural
                                                        lead-in to additional content. This content is a little bit
                                                        longer.</p>

                                                    <button type="button" class="btn btn-sm btn-outline-primary my-2">
                                                        <i class="fa fa-list mr-1" aria-hidden="true"></i>View Detail
                                                    </button>
                                                    <button type="button" class="btn btn-sm btn-outline-danger my-2">
                                                        <i class="fa fa-heart mr-1" aria-hidden="true"></i>Add to
                                                        Watchlist
                                                    </button>

                                                </div>
                                                <div class="card-footer">
                                                    <small class="text-muted">Last updated 3 mins ago</small>
                                                </div>
                                            </div>
                                        </div>
                                    {{/each}}

                                </div>
                                <button class="carousel-control-prev" type="button"
                                        data-target="#carouselExampleCaptions"
                                        data-slide="prev">
                                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                    <span class="sr-only">Previous</span>
                                </button>
                                <button class="carousel-control-next" type="button"
                                        data-target="#carouselExampleCaptions"
                                        data-slide="next">
                                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                    <span class="sr-only">Next</span>
                                </button>
                            </div>

                        </div>
                    </div>
                </div>
                <div class="col-lg-1"></div>
            </div>
        </div>
    </div>
    <!-- End of Top End -->

    <!-- Top Bid -->
    <div class="container-fluid py-3">
        <div class="row">
            <div class="col-lg-1"></div>
            <div class="col-lg-10">

                <div class="card">
                    <div class="card-header">
                        <div class="d-flex justify-content-between">
                            <h4 class="">Highest Bid</h4>
                            <a class="text-success font-weight-bold ml-auto mr-0" href="#">See all
                                <i class="fa fa-arrow-right" aria-hidden="true"></i></a>
                        </div>
                    </div>

                    <div class="card-body">
                        <div class="card-deck">
                            {{#each this.top_bid}}
                                <div class="card h-100">
                                    <img class="card-img-top" src="./public/imgs/sp/{{ProID}}/main_thumbs.jpg"
                                         alt="{{ProName}}"
                                         title="{{ProName}}">

                                    <div class="card-body">

                                        <h5 class="card-title">{{ProName}}</h5>
                                        <h5 class="card-title text-success">{{{format_number Price}}}</h5>

                                        <p class="card-text">This is a wider card with supporting text below as a
                                            natural
                                            lead-in to additional content. This content is a little bit longer.</p>

                                        <button type="button" class="btn btn-sm btn-outline-primary my-2">
                                            <i class="fa fa-list mr-1" aria-hidden="true"></i>View Detail
                                        </button>
                                        <button type="button" class="btn btn-sm btn-outline-danger my-2">
                                            <i class="fa fa-heart mr-1" aria-hidden="true"></i>Add to Watchlist
                                        </button>

                                    </div>
                                    <div class="card-footer">
                                        <small class="text-muted">Last updated 3 mins ago</small>
                                    </div>
                                </div>
                            {{/each}}
                        </div>
                    </div>
                </div>
                <div class="col-lg-1"></div>
            </div>
        </div>
    </div>
    <!-- End of Top Bid -->

    <!-- Top Price -->
    <div class="container-fluid py-3">
        <div class="row">
            <div class="col-lg-1"></div>
            <div class="col-lg-10">

                <div class="card">
                    <div class="card-header">
                        <div class="d-flex justify-content-between">
                            <h4 class="">Highest Price</h4>
                            <a class="text-success font-weight-bold ml-auto mr-0" href="#">See all
                                <i class="fa fa-arrow-right" aria-hidden="true"></i></a>
                        </div>
                    </div>

                    <div class="card-body">
                        <div class="card-deck">
                            {{#each this.top_price}}
                                <div class="card h-100">
                                    <img class="card-img-top" src="./public/imgs/sp/{{ProID}}/main_thumbs.jpg"
                                         alt="{{ProName}}"
                                         title="{{ProName}}">
                                    <div class="card-body">

                                        <h5 class="card-title">{{ProName}}</h5>
                                        <h5 class="card-title text-success">{{{format_number Price}}}</h5>

                                        <p class="card-text">This is a wider card with supporting text below as a
                                            natural
                                            lead-in to additional content. This content is a little bit longer.</p>

                                        <button type="button" class="btn btn-sm btn-outline-primary my-2">
                                            <i class="fa fa-list mr-1" aria-hidden="true"></i>View Detail
                                        </button>
                                        <button type="button" class="btn btn-sm btn-outline-danger my-2">
                                            <i class="fa fa-heart mr-1" aria-hidden="true"></i>Add to Watchlist
                                        </button>

                                    </div>
                                    <div class="card-footer">
                                        <small class="text-muted">Last updated 3 mins ago</small>
                                    </div>
                                </div>
                            {{/each}}
                        </div>
                    </div>
                </div>
                <div class="col-lg-1"></div>
            </div>
        </div>
    </div>
    <!-- End of Top Price -->

    <!-- End of Main Content -->
</div>

